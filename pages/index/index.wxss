page{
  width: 100%;
  height: 100%;
}
.header{
  width: 90%;
  height: 10%;
  margin-left: 5%;
  display: flex;
  flex-direction: row;
}
.search_box{
  width: 100%;
  height: 52rpx;
  display: flex;
  flex-direction: row;
  background: #f5f5f5;
  border-radius: 26rpx;
  align-self: center;
}
.search_box_type{
  width: 36%;
  height: 100%;
  display: flex;
  flex-direction: row;
  margin-left: 32%;
  align-self: center;
}
.search_box_type text{
  color:#acacac;
  font-size: 26rpx;
  line-height: 52rpx;
  margin-left: 20rpx;
  align-self: center;
}
.search_box_type image{
  width: 32rpx;
  height: 32rpx;
  align-self: center;
}




.contain{
  width: 66%;
  height: 42%;
  margin-left: 17%;
  margin-top: 5%;
}


.contain_top,.contain_bottom{
  width: 100%;
  height: 50%;
  display: flex;
  flex-direction:column;
}
.contain_top_top,.contain_bottom_top{
  width: 100%;
  height: 38%;
  display: flex;
  flex-direction: row;
}
.icon{
  color: #5ed1de;
  font-size: 46rpx;
  font-weight: bold;
  align-self: center;
}
.testtext{
  font-size: 42rpx;
  align-self: center;
  margin-left: 3%;
  font-weight: bold;
}

.contain_top_bottom,.contain_bottom_bottom{
  width: 100%;
  height: 62%;
}
.contain_top_bottom image{
  width: 122rpx;
  height: 100%;
  float: right;
}
.contain_bottom_bottom image{
  width: 122rpx;
  height: 100%;
  float: right;
}




.footer{
  width: 84%;
  height: 35%;
  margin-left: 8%;
  margin-top: 5%;
}
.footer_top,.footer_bottom{
  width: 100%;
  height: 50%;
  display: flex;
  flex-direction: column;
}
.footer_top_top,.footer_bottom_top{
  width: 100%;
  height: 60%;
  display: flex;
  flex-direction: row;
}
.Topimg{
  width: 70rpx;
  height: 70rpx;
  align-self: center;
  margin-top: 28rpx;
}
.top_box{
  width: 33.33%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-self: center;
} 
.footer_top_bottom,.footer_bottom_bottom{
  width: 100%;
  height: 40%;
  display: flex;
  flex-direction: row;
}
.fonts_box{
  width: 33.33%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-self: center;
}
.fonts_box text{
  color:#656565;
  font-size:34rpx;
  font-family: Arial, Helvetica, sans-serif;
  align-self: center;
}


.box1{
  margin-top: 5rpx;
  position: relative;
  border-style: solid;
  border-color: #5ed1de;
  border-radius: 26rpx;
  align-items: center;
  justify-content: center;
  text-align: center;
}